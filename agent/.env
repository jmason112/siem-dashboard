# Agent Configuration
AGENT_ID=windows-agent-1
AGENT_SECRET=default-secret
API_URL=http://localhost:3000
SCAN_INTERVAL=3600

# Monitoring Configuration
CPU_THRESHOLD=80
MEMORY_THRESHOLD=80
DISK_THRESHOLD=90

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=agent.log

# Security Configuration
MONITORED_DIRECTORIES=/etc,/var/log
EXCLUDED_PROCESSES=system,idle
ALLOWED_PORTS=80,443,3000,3001 

API_KEY=your_api_key_here